{
  "manifest_version": 2,
  "name": "Adblock Fast",
  "version": "1.1.0",
  "description":
      "The worldâ€™s fastest ad blocker! Same ad blocking, 8x accelerated page loading.",
  "icons": {
    "16": "chrome/images/16.png",
    "48": "chrome/images/48.png",
    "128": "chrome/images/128.png"
  },
  "developer": {"name": "Rocketship", "url": "http://rocketshipapps.com/"},
  "permissions": [
    "activeTab",
    "alarms",
    "browsingData",
    "clipboardWrite",
    "contextMenus",
    "cookies",
    "declarativeContent",
    "fontSettings",
    "identity",
    "idle",
    "notifications",
    "pageCapture",
    "power",
    "storage",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy":
      "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "chrome/scripts/host.js",
      "chrome/scripts/blocking.js",
      "chrome/scripts/background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": ["chrome/stylesheets/content.css"],
    "js": [
      "chrome/scripts/host.js",
      "chrome/scripts/blocking.js",
      "chrome/scripts/hiding.js",
      "chrome/scripts/content.js"
    ],
    "run_at": "document_start",
    "all_frames": true
  }],
  "web_accessible_resources": [
      "assets/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "chrome/images/blocked/19.png", "38": "chrome/images/blocked/38.png"
    },
    "default_title": "Unblock ads on this site"
  }
}
